<% content_for :footer_scripts do %>
<script>
  <% if flash[:swal] %>
    swal({
        title: "<%= flash[:swal][0] %>",
        text: "<%= flash[:swal][1] %>",
        type: "<%= flash[:swal][2] %>",
        showCancelButton: true,
        confirmButtonColor: "#DD6B55",
        confirmButtonText: "Aceptar",
        cancelButtonText: "Cancelar",
        closeOnConfirm: true,
        closeOnCancel: true
      });
  <% end %>
</script>
<%end%>
